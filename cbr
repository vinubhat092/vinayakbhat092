#include<stdio.h>
void input(int *a,int *b)
{
    printf("enter two numbures *a,*b);
    scanf("%d%d"&a,&b);
}
void add(int a,int b,int *s)
{
    *s=a=b;
    return *s;
}
void output(int *s)
{
    printf("out put is %d\n");
}
int main()
{
    int a,b;
    *s=a+b;
    int add(*a,*b);
    return *s;
 }
